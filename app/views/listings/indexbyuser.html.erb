<h1>Your Listings</h1>

<table>
  <tr>
    <th>Category</th>
    <th>Title</th>
     <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.category_id %></td>
    <td><%= listing.title %></td>
    <td></td>
    <td><%= link_to 'Show', {:action => 'show', :id => listing.id} %></td>
    <td><%= link_to 'Edit', {:action => 'edit', :id => listing.id} %></td>
    <td><%= link_to 'Delete', {:action => 'delete', :id => listing.id}, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

